{
	"name": "@essential-js/sender",
	"version": "0.0.1",
	"title": "Essential mailer",
	"description": "Essential sender is a package that seeks through a minimalist api to provide an efficient and simple service for sending emails in all your applications.",
	"license": "ISC",
	"languages": {
		"default": "en",
		"supported": [
			"en",
			"es"
		]
	},
	"modules": {
		"path": "modules"
	},
	"deployment": {
		"distributions": [
			{
				"name": "node",
				"environment": "development",
				"platform": "node",
				"ports": {
					"bundles": 6560,
					"inspect": 6561
				},
				"default": true,
				"ts": {
					"compiler": "tsc"
				}
			},
			{
				"name": "backend",
				"environment": "development",
				"platform": "backend",
				"ports": {
					"bundles": 7081,
					"http": 7082,
					"inspect": 7083
				}
			}
		]
	},
	"dependencies": {
		"@beyond-js/kernel": "~0.1.8",
		"@beyond-js/local": "~0.1.3",
		"@beyond-js/react-18-widgets": "^0.0.3",
		"@beyond-js/react-widgets": "^18.21.1",
		"handlebars": "^4.7.7",
		"nodemailer": "^6.9.1",
		"socket.io-client": "^4.5.4"
	},
	"devDependencies": {
		"@types/handlebars": "^4.1.0",
		"@types/node": "^16.11.7",
		"@types/nodemailer": "^6.4.7",
		"@types/react": "^18.0.27",
		"@types/react-dom": "^18.0.10"
	}
}
